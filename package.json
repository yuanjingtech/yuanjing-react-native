{
  "name": "yuanjing",
  "version": "1.3.0",
  "private": true,
  "scripts": {
    "start": "react-native start",
    "ios": "react-native run-ios --simulator=\"iPhone 11 Pro Max\"",
    "ios:bundle": "react-native bundle --entry-file index.js --bundle-output ./ios/main.jsbundle --platform ios --assets-dest ./ios --dev false",
    "ios:release": "react-native run-ios --configuration Release --simulator=\"iPhone 11\"",
    "ios:codepush": "appcenter codepush release-react -a yuanjingtech/yuanjing -d Staging",
    "android": "react-native run-android",
    "android:reverse": "adb reverse tcp:8081 tcp:8081",
    "android:bundle": "react-native bundle --entry-file index.android.js --bundle-output ./android/app/src/main/assets/index.android.bundle --platform android --assets-dest ./android/app/src/main/res --dev false",
    "android:release": "react-native run-android --variant=release",
    "android:codepush": "appcenter codepush release-react -a yuanjingtech/yuanjing-1 -d Staging",
    "codepush": "yarn run android:codepush && yarn run ios:codepush",
    "test": "jest",
    "lint": "eslint ."
  },
  "dependencies": {
    "@apollo/react-hooks": "^3.1.3",
    "@react-native-community/async-storage": "^1.6.3",
    "@react-native-community/masked-view": "^0.1.5",
    "@react-native-firebase/analytics": "^6.2.0",
    "@react-native-firebase/app": "^6.2.0",
    "@react-navigation/bottom-tabs": "^5.0.0-alpha.22",
    "@react-navigation/compat": "^5.0.0-alpha.17",
    "@react-navigation/material-bottom-tabs": "^5.0.0-alpha.25",
    "@react-navigation/native": "^5.0.0-alpha.19",
    "@react-navigation/stack": "^5.0.0-alpha.44",
    "add": "^2.0.6",
    "apollo-cache-inmemory": "^1.6.3",
    "apollo-client": "^2.6.4",
    "apollo-link": "^1.2.13",
    "apollo-link-context": "^1.0.19",
    "apollo-link-error": "^1.1.12",
    "apollo-link-http": "^1.5.16",
    "appcenter": "^2.2.0",
    "appcenter-analytics": "^2.2.0",
    "appcenter-crashes": "^2.2.0",
    "appcenter-push": "^2.2.0",
    "fbemitter": "^2.1.1",
    "graphql": "^14.5.8",
    "graphql-tag": "^2.10.1",
    "hermesvm": "^0.1.1",
    "moment": "^2.24.0",
    "prop-types": "^15.7.2",
    "ramda": "^0.26.1",
    "react": "16.9.0",
    "react-native": "0.61.1",
    "react-native-admob": "^2.0.0-beta.6",
    "react-native-appearance": "^0.3.1",
    "react-native-code-push": "^5.6.1",
    "react-native-gesture-handler": "^1.5.2",
    "react-native-material-ui": "^1.30.1",
    "react-native-paper": "^3.3.0",
    "react-native-reanimated": "^1.4.0",
    "react-native-safe-area-context": "^0.6.1",
    "react-native-screens": "^1.0.0-alpha.23",
    "react-native-scrollable-tab-view": "^1.0.0",
    "react-native-super-grid": "^3.1.1",
    "react-native-vector-icons": "^6.6.0",
    "react-native-viewpager": "^0.2.13",
    "react-native-webview": "^6.9.0",
    "reactotron-react-native": "^3.6.5",
    "rongcloud-react-native-imlib": "^0.3.3"
  },
  "devDependencies": {
    "@babel/core": "^7.5.5",
    "@babel/plugin-proposal-nullish-coalescing-operator": "^7.7.4",
    "@babel/plugin-proposal-optional-chaining": "^7.7.5",
    "@babel/runtime": "^7.5.5",
    "@react-native-community/eslint-config": "^0.0.5",
    "@types/fbemitter": "^2.0.32",
    "@types/jest": "^24.0.17",
    "@types/ramda": "^0.26.36",
    "@types/react": "^16.9.1",
    "@types/react-native": "^0.60.3",
    "@types/react-native-material-ui": "^1.31.1",
    "@types/react-native-scrollable-tab-view": "^0.10.0",
    "@types/react-test-renderer": "^16.9.0",
    "babel-jest": "^24.8.0",
    "eslint": "^6.1.0",
    "jest": "^24.8.0",
    "metro-react-native-babel-preset": "^0.55.0",
    "react-addons-test-utils": "^15.6.2",
    "react-native-typescript-transformer": "^1.2.13",
    "react-test-renderer": "16.8.6",
    "ts-jest": "^24.0.2",
    "typescript": "^3.5.3"
  },
  "jest": {
    "preset": "react-native",
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js"
    ],
    "transform": {
      "^.+\\.(js)$": "<rootDir>/node_modules/babel-jest",
      "\\.(ts|tsx)$": "<rootDir>/node_modules/ts-jest/preprocessor.js"
    },
    "testRegex": "(/__tests__/.*|\\.(test|spec))\\.(ts|tsx|js)$",
    "testPathIgnorePatterns": [
      "\\.snap$",
      "<rootDir>/node_modules/"
    ],
    "cacheDirectory": ".jest/cache"
  }
}
